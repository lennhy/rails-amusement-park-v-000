<table>
<% @attractions.each do |attraction| %>
  <tr>
   <td> <%= attraction.name %> </td>
   <td> <%= attraction.tickets %> </td>
   <td> <%=link_to "Go on #{attraction.name}", attraction_path(attraction) %> </td>
  </tr>
 <% end %>
</table>
<% if @user %>
  <p>
    <%= link_to "Back to profile", user_path(@user), {:class => "btn btn-primary"} %>
  </p>
<% else %>
  <p><%= link_to 'New Attraction', new_attraction_path %></p>
<% end %>
